import java.util.Scanner;

public class Main {
    public static void main(String argv[]) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        System.out.println(cal(N));
    }
    public static int cal(int n){
        if(n==0){return 0;}
        else if (n == 1) {return 1;}
        return cal(n-1) + cal(n-2);
    }
}
