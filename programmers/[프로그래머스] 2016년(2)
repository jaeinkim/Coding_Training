class Solution {
public String solution(int a, int b) {
        String answer = "";
        String[] day = {"THU", "FRI", "SAT", "SUN", "MON", "TUE", "WED"};
        int[] month = {31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
        for(int i = 1 ; i < a; i++){
            b += month[i-1];
        }
//        System.out.println(day[b%7] );
        return day[b%7] ;
    }
}
