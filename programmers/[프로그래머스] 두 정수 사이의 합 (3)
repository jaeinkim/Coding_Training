public class test2 {
    public static void main(String[] argv){
        test2 te = new test2();
        te.solution(3,5);
    }
    public long solution(int a, int b) {
        //5 3  -> 12
        long answer = 0;
        for(int i = Math.min(a, b);i <= Math.max(a, b);i++){
            answer += i;
        }
        return answer;
    }
}
